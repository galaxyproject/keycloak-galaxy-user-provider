      "secret": "test-client-secret",
      "redirectUris": ["http://localhost:8080/*"],
      "webOrigins": ["*"],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "fullScopeAllowed": true,
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "basic",
        "profile",
        "roles",
        "email"
      ]
    }
  ],
  "users": [
    {
      "username": "kcuser",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Keycloak",
      "lastName": "User",
      "email": "kcuser@test.org",
      "credentials": [
        {
          "type": "password",
          "value": "kcpass",
          "temporary": false
        }
      ]
    }
  ]
